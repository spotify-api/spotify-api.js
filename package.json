{
    "name": "spotify-api.js",
    "version": "9.0.3",
    "description": "A complete node js wrapper of spotify api with oauth support",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/spotify-api/spotify-api.js"
    },
    "homepage": "https://github.com/spotify-api/spotify-api.js#readme",
